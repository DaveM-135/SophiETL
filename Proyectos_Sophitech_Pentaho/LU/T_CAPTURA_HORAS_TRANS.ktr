    <sql>SELECT
  id_dia
--, id_mes
, id_semana
--, desc_dia
, fec_dia_fecha
--, sk_dia
--, val_habil
, val_horas_habiles
, val_hrs_feriados 
FROM dbo.LU_dia
where (val_horas_habiles > 0 or val_hrs_feriados > 0) -- feriados y habiles 
and DATEPART(dw,fec_dia_fecha) not in (1,7) --excluye sabado y domingo
and DATEPART(YYYY,fec_dia_fecha) in ( DATEPART(YYYY, dateadd(month, -1, getdate())) -- A単o mes previo
,DATEPART(YYYY,getdate())) -- a単o actual 
and DATEPART(ISO_WEEK, fec_dia_fecha)  between 
DATEPART(ISO_WEEK, (DATEADD(mm, DATEDIFF(mm, 0, GETDATE()) - 1, 0))) and  -- primera semana del mes anterior
DATEPART(ISO_WEEK,(DATEADD(DAY, -(DAY(GETDATE())), GETDATE()))) -- ultima semana del mes anterior</sql>
